{
  // デフォルト: パターン2（Claude節約構成）
  "description": "デフォルト: Claude節約（パターン2）",
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro", // 指揮はGemini Proで節約
      "variant": "high",
      "temperature": 0.2,
    },
    "hephaestus": {
      "model": "openai/gpt-5.2-codex", // 職人は妥協せずCodex
    },
    "oracle": {
      "model": "google/antigravity-claude-opus-4-5-thinking", // ここぞという相談にのみClaude Opusを使用
    },
    "momus": {
      "model": "openai/gpt-5.2", // レビューはGPT
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash",
    },
  },

  "categories": {
    "quick": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "writing": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max",
    },

    "ultrabrain": {
      "model": "openai/gpt-5.2-codex",
      "variant": "xhigh",
    },
    "deep": {
      "model": "openai/gpt-5.2-codex",
      "variant": "medium",
    },

    // Claudeは最小限：high だけ Opus に寄せる
    "unspecified-low": {
      "model": "openai/gpt-5.2",
    },
    "unspecified-high": {
      "model": "google/antigravity-claude-opus-4-5-thinking",
      "variant": "max",
    },
  }
}
