{
  // デフォルト: パターン2（Claude節約構成）
  "description": "デフォルト: Claude節約（パターン2）",
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro", // 指揮はGemini Proで節約
      "variant": "high",
      "temperature": 0.2,
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex", // 職人は妥協せずCodex
    },
    "oracle": {
      "model": "google/antigravity-claude-opus-4-6-thinking", // ここぞという相談にのみClaude Opusを使用
    },
    "momus": {
      "model": "openai/gpt-5.2", // レビューはGPT

    },
    "librarian": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash",
    },

    // 追加: Git操作専用の職人エージェント ▼▼▼
    "git-specialist": {
      "description": "Git operations specialist with elevated permissions",
      "model": "google/antigravity-gemini-3-pro", 
      "permission": {
        "bash": {
          // 破壊的コマンドはブロック
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          // それ以外のGit操作は全て許可
          "git *": "allow",
          // Git以外は実行禁止（セキュリティ担保）
          "*": "deny",
        },
        // コード編集やWeb閲覧も禁止
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },


  "categories": {
    "quick": {
      "model": "git-specialist",
    },
    "writing": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max",
    },

    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh",
    },
    "deep": {
      "model": "openai/gpt-5.3-codex",
      "variant": "medium",
    },

    // Claudeは最小限：high だけ Opus に寄せる
    "unspecified-low": {
      "model": "openai/gpt-5.2",
    },
    "unspecified-high": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max",
    },
  }
}
