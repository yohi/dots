{
  // 【パターン3】No Claude (GPT + Gemini)
  //  Claudeを使用せず、GPTとGeminiのみで構成。
  //  GPT-5.3 Codexの登場により、実装力は最強構成に引けを取らない。
  "description": "Claudeなし (GPT-5.3 + Gemini)",
  "agents": {
    "sisyphus": {
      "model": "google-vertex/gemini-3.1-pro-preview", // `no-sisyphus-gpt`ルールに準拠するため、指揮層(Sisyphus)にはGPTではなく最高性能のGemini Proを採用
      "variant": "high",
      "temperature": 0.2,
    },
    "prometheus": {
      "model": "openai/gpt-5.2", // 計画立案には特化型より幅広い文脈理解を持つ汎用推論モデルを採用
      "variant": "xhigh"
    },
    // Context Manager / Architect
    "atlas": {
      "model": "google-vertex/gemini-3.1-pro-preview", // 1Mの大規模コンテキストを活かすためGemini Proを採用
      "variant": "high"
    },
    "metis": {
      "model": "openai/gpt-5.2", // 参謀：GPT-5.2
      "temperature": 0.5
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex", // 実装は当然Codex
      "variant": "high"
    },
    "oracle": {
      "model": "openai/gpt-5.2", // 相談役はバランスの良いGPT-5.2
    },
    "momus": {
      "model": "google-vertex/gemini-3.1-pro-preview", // 異種モデルによる多角的かつ客観的なレビューのためGeminiを採用
    },
    "librarian": {
      "model": "google-vertex/gemini-3-flash-preview",
    },
    "explore": {
      "model": "google-vertex/gemini-3-flash-preview",
    },
    "multimodal-looker": {
      "model": "google-vertex/gemini-3-flash-preview",
    },
    "git-specialist": {
      "description": "Git operations specialist",
      "model": "openai/gpt-5.3-codex",
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },
  "categories": {
    "quick": {
      "model": "google-vertex/gemini-3-flash-preview"
    },
    "writing": {
      "model": "google-vertex/gemini-3-flash-preview"
    },
    "visual-engineering": {
      "model": "google-vertex/gemini-3.1-pro-preview"
    },
    "artistry": {
      "model": "google-vertex/gemini-3.1-pro-preview",
      "variant": "high"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh"
    },
    "deep": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "unspecified-low": {
      "model": "openai/gpt-5.2"
    },
    "unspecified-high": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
  }
}