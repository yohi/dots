{
  // 【パターン3】No Claude (GPT + Gemini)
  //  Claudeを使用せず、GPTとGeminiのみで構成。
  //  GPT-5.3 Codexの登場により、実装力は最強構成に引けを取らない。
  "description": "Claudeなし (GPT-5.3 + Gemini)",
  "agents": {
    "sisyphus": {
      "model": "openai/gpt-5.3-codex", // 指揮もGPT-5.3 (Terminal操作が強いため)
      "variant": "high",
      "temperature": 0.1,
    },
    "metis": {
      "model": "openai/gpt-5.2", // 参謀：GPT-5.2
      "temperature": 0.5
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex", // 実装は当然Codex
      "variant": "high"
    },
    "oracle": {
      "model": "openai/gpt-5.2", // 相談役はバランスの良いGPT-5.2
    },
    "momus": {
      "model": "openai/gpt-5.2",
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "git-specialist": {
      "description": "Git operations specialist",
      "model": "openai/gpt-5.3-codex",
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },

  "categories": {
    "quick": { "model": "git-specialist" },
    "writing": { "model": "google/antigravity-gemini-3-flash" },
    "visual-engineering": { "model": "google/antigravity-gemini-3-pro" },
    "artistry": { "model": "google/antigravity-gemini-3-pro", "variant": "high" },
    "ultrabrain": { "model": "openai/gpt-5.3-codex", "variant": "xhigh" },
    "deep": { "model": "openai/gpt-5.3-codex", "variant": "high" },
    "unspecified-low": { "model": "openai/gpt-5.2" },
    "unspecified-high": { "model": "openai/gpt-5.3-codex", "variant": "high" },
  }
}
