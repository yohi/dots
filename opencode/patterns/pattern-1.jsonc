{
  // 【パターン1】GPT + Claude + Gemini (最強構成: Best-of-Breed) [DEFAULT]
  //  各社の最強モデルを適材適所で利用。SisyphusにOpus、職人にCodex、探索にGemini。
  "description": "GPT+Claude+Gemini 最強構成（Sisyphus=Opus, Hephaestus=Codex）",
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-claude-opus-4-6-thinking", // 指揮・推論には最強のOpus (Thinking)
      "variant": "max",
      "temperature": 0.2,
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex", // [必須] 深い実装を行う職人エージェント
    },
    "oracle": {
      "model": "openai/gpt-5.2", // 相談役：論理的レビュー・デバッグ
    },
    "momus": {
      "model": "openai/gpt-5.2", // 品質保証：コードレビュー
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-pro", // 書庫番：長文ドキュメント・検索
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash", // 探索：高速なコードベース把握
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash", // 視覚：画像・PDF認識
    },
  },

  // ★移行後カテゴリ：built-in のみ（visual/business-logic を廃止）
  "categories": {
    "quick": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "writing": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max",
    },

    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh",
    },
    "deep": {
      "model": "openai/gpt-5.3-codex",
      "variant": "medium",
    },

    "unspecified-low": {
      "model": "google/antigravity-claude-sonnet-4-5",
    },
    "unspecified-high": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max",
    },
  }
}
