{
  // 【パターン1】Winning Stack (最強構成: Best-of-Breed)
  //  レポート推奨の最強構成。SisyphusにOpus、職人にGPT-5.3、検索にGemini Flash。
  //  設計から実装まで妥協のない最高性能を発揮します。
  "description": "最強構成 (Opus 4.6 + GPT-5.3 + Gemini Flash)",
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-claude-opus-4-6-thinking", // 指揮・推論には最強のOpus (Thinking)
      "variant": "max",
      "temperature": 0.1,
      "thinking": {
        "budget": 32768
      }
    },
    "metis": {
      "model": "google/antigravity-claude-opus-4-6-thinking", // 参謀：計画の抜け漏れをOpusで徹底レビュー
      "variant": "max",
      "temperature": 0.5
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex", // 実装：Terminal操作最強のGPT-5.3
      "variant": "high"
    },
    "oracle": {
      "model": "google/antigravity-claude-opus-4-6-thinking", // 相談役：深い推論が必要なためOpus
      "variant": "max"
    },
    "momus": {
      "model": "google/antigravity-claude-sonnet-4-5", // レビュー：Sonnetで十分高品質
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash", // 書庫番：圧倒的コスパのGemini Flash
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash", // 探索：高速なコードベース把握
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash", // 視覚：画像・PDF認識
    },
    
    // 特権エージェント（Git操作）
    "git-specialist": {
      "description": "Git operations specialist with elevated permissions",
      "model": "openai/gpt-5.3-codex", // 複雑なGit操作もGPT-5.3に任せる
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },

  "categories": {
    "quick": {
      "model": "git-specialist",
    },
    "writing": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "visual-engineering": {
      "model": "google/antigravity-claude-sonnet-4-5", // UI実装はSonnet 4.5が優秀
    },
    "artistry": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max",
    },
    "ultrabrain": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max",
    },
    "deep": {
      "model": "openai/gpt-5.3-codex", // 深い実装はGPT-5.3
      "variant": "high",
    },
    "unspecified-low": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "unspecified-high": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max",
    },
  }
}
