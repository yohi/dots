    "git-specialist": {
      "description": "Git operations specialist",
      "model": "google-vertex/gemini-3-flash-preview",
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },
