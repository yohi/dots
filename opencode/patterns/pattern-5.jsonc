{
  // 【パターン5】Ultra Low Cost (激安構成)
  //  ほぼ全てをGemini系で固めた構成。大量のログ解析や試行錯誤向け。
  "description": "激安構成 (All Gemini)",
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
      "temperature": 0.2,
    },
    "metis": {
      "model": "google/antigravity-gemini-3-pro", // 参謀もGemini Pro
      "variant": "high",
      "temperature": 0.5
    },
    // Hephaestus (GPT) は使用不可 -> Sisyphusにフォールバック
    "oracle": {
      "model": "google/antigravity-gemini-3-pro", // 相談もGemini
      "variant": "high"
    },
    "momus": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "git-specialist": {
      "description": "Git operations specialist",
      "model": "google/antigravity-gemini-3-flash",
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },

  "categories": {
    "quick": { "model": "git-specialist" },
    "writing": { "model": "google/antigravity-gemini-3-flash" },
    "visual-engineering": { "model": "google/antigravity-gemini-3-pro" },
    "artistry": { "model": "google/antigravity-gemini-3-pro" },
    "ultrabrain": { "model": "google/antigravity-gemini-3-pro" },
    "deep": { "model": "google/antigravity-gemini-3-pro" },
    "unspecified-low": { "model": "google/antigravity-gemini-3-flash" },
    "unspecified-high": { "model": "google/antigravity-gemini-3-pro" },
  }
}
