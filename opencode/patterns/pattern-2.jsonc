    "momus": {
      "model": "google-vertex/gemini-3.1-pro-preview",
    },
    "librarian": {
      "model": "google-vertex/gemini-3-flash-preview",
    },
    "explore": {
      "model": "google-vertex/gemini-3-flash-preview",
    },
    "multimodal-looker": {
      "model": "google-vertex/gemini-3-flash-preview",
    },
    "git-specialist": {
      "description": "Git operations specialist",
      "model": "google-vertex/gemini-3.1-pro-preview",
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },
