    },
    "atlas": {
      "model": "google-vertex/gemini-3.1-pro-preview", // ContextもGemini Pro
      "variant": "high"
    },
    "metis": {
      "model": "openai/gpt-5.2", // 参謀：バランス型のGPT-5.2
