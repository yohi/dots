{
  // 【パターン2】Economy (コスパ・節約構成)
  //  SisyphusをGemini 3 Proに任せてコストを抑制しつつ、要所(実装・相談)で高性能モデルを使う。
  "description": "節約構成 (Gemini Pro + GPT-5.3 + Flash)",
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro", // 指揮はGemini Pro
      "variant": "high",
      "temperature": 0.2,
    },
    "metis": {
      "model": "openai/gpt-5.2", // 参謀：バランス型のGPT-5.2
      "temperature": 0.5
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex", // 実装だけは妥協せずGPT-5.3
      "variant": "medium"
    },
    "oracle": {
      "model": "google/antigravity-claude-opus-4-6-thinking", // 相談役だけはOpus（ここぞという時用）
      "variant": "max"
    },
    "momus": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash",
    },
    "git-specialist": {
      "description": "Git operations specialist",
      "model": "google/antigravity-gemini-3-pro",
      "permission": {
        "bash": {
          "git push --force": "deny",
          "git push -f": "deny",
          "git clean *": "ask",
          "git add .": "ask",
          "git *": "allow",
          "*": "deny",
        },
        "edit": "deny",
        "webfetch": "deny",
      },
    },
  },

  "categories": {
    "quick": { "model": "git-specialist" },
    "writing": { "model": "google/antigravity-gemini-3-flash" },
    "visual-engineering": { "model": "google/antigravity-gemini-3-pro" },
    "artistry": { "model": "google/antigravity-gemini-3-pro", "variant": "high" },
    "ultrabrain": { "model": "google/antigravity-gemini-3-pro", "variant": "high" },
    "deep": { "model": "openai/gpt-5.3-codex", "variant": "medium" }, // deepだけはGPT
    "unspecified-low": { "model": "google/antigravity-gemini-3-flash" },
    "unspecified-high": { "model": "google/antigravity-gemini-3-pro" },
  }
}
