{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "disabled_hooks": [
    "startup-toast"
  ],
  "disabled_mcps": [],
  "agents": {
    "atlas": {
      "model": "google/antigravity-gemini-3-pro-high",
      "temperature": 0.5,
      "description": "プロジェクト全体を統括するリーダー。タスク分解、委譲、進捗管理を担当。"
    },
    "prometheus": {
      "category": "ultrabrain",
      "variant": "high",
      "temperature": 0.3,
      "description": "実装前の詳細な計画策定を担当。依存関係の解決とリスク予測を行う。"
    },
    "metis": {
      "model": "google/antigravity-gemini-3-pro-high",
      "variant": "high",
      "temperature": 0.3,
      "description": "計画に対する批判的分析、隠れた要件の抽出、リスク評価を担当。"
    },
    "sisyphus": {
      "model": "openai/gpt-5.2-codex",
      "temperature": 0.5
    },
    "sisyphus-junior": {
      "model": "google/antigravity-gemini-3-pro-low",
      "temperature": 0.1,
      "description": "Orchestratorから委譲されるタスクを実行する実務担当。実装のみに集中する。"
    },
    "frontend-ui-ux-engineer": {
      "model": "google/antigravity-gemini-3-pro-high",
      "temperature": 0.4,
      "description": "UIコンポーネントの実装、CSS調整、UX改善、レスポンシブ対応を担当。"
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
      "temperature": 0.1,
      "description": "広範なドキュメント検索、ライブラリ仕様の確認、コードベースの歴史的経緯の調査。"
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
      "temperature": 0.1
    },
    "oracle": {
      "category": "ultrabrain",
      "variant": "xhigh",
      "temperature": 0.5,
      "description": "難解な問題に対する深い洞察、アーキテクチャ設計、解決策の提示。"
    },
    "momus": {
      "model": "openai/gpt-5.2-codex",
      "temperature": 0.2,
      "description": "コードレビュー、セキュリティ監査、品質チェック、エッジケースの指摘。"
    },
    "document-writer": {
      "model": "google/antigravity-gemini-3-flash",
      "temperature": 0.3
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-pro-high",
      "temperature": 0.2
    }
  },
  "sisyphus_agent": {
    "disabled": false,
    "default_builder_enabled": false,
    "planner_enabled": true,
    "replace_plan": true
  },
  "background_task": {
    "defaultConcurrency": 5
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "ralph_loop": {
    "enabled": true,
    "default_max_iterations": 50
  }
}
