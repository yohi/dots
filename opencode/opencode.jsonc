{

  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@3.0.0-beta.10",
    // "oh-my-opencode@v3.0.0-beta.8",
    // "opencode-openai-codex-auth@latest",
    "opencode-antigravity-auth@beta",
    "@yohi/opencode-antigravity-quota",
    "@yohi/opencode-codex-quota"
  ],

  // [Permission]
  "permission": {
    // 【読み取り・検索系】: ステートを変更しないため常時許可推奨
    "read": "allow",
    "list": "allow",
    "glob": "allow",
    "grep": "allow",
    "codesearch": "allow",
    "websearch": "allow",
    "todoread": "allow",

    // 【ファイル編集】: 開発効率のため基本許可（ここが "ask" だと毎回止まります）
    "edit": "allow",
    "todowrite": "allow",

    // 【外部アクセス・危険領域】: 誤動作防止のため確認を入れる
    "webfetch": "ask",
    "external_directory": "ask", // プロジェクト外のファイルアクセスは確認

    // 【コマンド実行】: よく使う安全なコマンドのみ許可、破壊的なものは禁止、その他は確認
    "bash": {
      "*": "ask",              // デフォルトは確認する
      "ls": "allow",           // ディレクトリ確認は許可
      "ls *": "allow",
      "cat *": "allow",        // ファイル読み込みは許可
      "grep *": "allow",       // 検索は許可
      "find *": "allow",
      "git status": "allow",   // gitの状態確認は許可
      "git diff": "allow",
      "git log": "allow",
      "npm test": "allow",     // テスト実行は許可（状況に応じて）
      "npm run lint": "allow",
      "echo *": "allow",       // エコーは許可
      "rm *": "deny",          // 削除コマンドは明示的に禁止（AIに提案させない）
      "rm -rf *": "deny",
      "ssh *": "deny"          // 外部接続は禁止
    }
  },

  // [MCP]
  "mcp": {
    "sequential-thinking": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "magic": {
      "type": "local",
      "command": [
        "npx",
        "@21st-dev/magic"
      ]
    },
    "playwright": {
      "type": "local",
      "command": [
        "npx",
        "@playwright/mcp@latest"
      ]
    },
    "morphllm-fast-apply": {
      "type": "local",
      "command": [
        "npx",
        "@morph-llm/morph-fast-apply"
      ]
    },
    "serena": {
      "type": "local",
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "tavily": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "tavily-mcp@latest"
      ],
      "environment": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "chrome-devtools": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "chrome-devtools-mcp@latest"
      ]
    }
  },
