{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@latest",
    "opencode-openai-codex-auth@latest",
    "@yohi/opencode-codex-quota@latest"
  ],
  "instructions": [],
  "watcher": {
    "ignore": [
      "**/node_modules/**",
      "**/dist/**",
      "**/.next/**",
      "**/coverage/**",
      "**/*.lock",
      "**/.cursorrules"
    ]
  },
  "compaction": {
    "auto": true,
    "reserved": 10000,
    "prune": true
  },
  "permission": {
    "read": "allow",
    "list": "allow",
    "glob": "allow",
    "grep": "allow",
    "codesearch": "allow",
    "todoread": "allow",
    "edit": "allow",
    "todowrite": "allow",
    "websearch": "allow",
    "webfetch": "ask",
    "external_directory": "ask",
    "bash": {
      "*": "ask",
      "ls*": "allow",
      "cat*": "allow",
      "pwd": "allow",
      "echo*": "allow",
      "mkdir*": "allow",
      "touch*": "allow",
      "mv*": "ask",
      "cp*": "ask",
      "grep*": "allow",
      "wc*": "allow",
      "find*": "allow",
      "tail*": "allow",
      "head*": "allow",
      "less*": "allow",
      "xargs*": "deny",
      "git *": "allow",
      "npm *": "allow",
      "yarn *": "allow",
      "node *": "allow",
      "python *": "allow",
      "bun *": "allow",
      "coderabbit *": "allow",
      "git push*": "ask",
      "git commit*": "ask",
      "git rebase*": "ask",
      "git merge*": "ask",
      "git checkout*": "allow",
      "git branch*": "allow",
      "git log*": "allow",
      "git status": "allow",
      "git diff*": "allow",
      "git show*": "allow",
      "git restore*": "ask",
      "git reset*": "ask",
      "git switch*": "allow",
      "git stash*": "allow",
      "npm publish*": "deny",
      "git add .": "deny",
      "rm *": "deny",
      "rm -rf *": "deny",
      "ssh *": "deny",
      "sudo *": "deny",
      "su *": "deny"
    }
  },
  "mcp": {
    "docker": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@docker/mcp-server"
      ]
    },
    "sequential-thinking": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "magic": {
      "type": "local",
      "command": [
        "npx",
        "@21st-dev/magic"
      ]
    },
    "playwright": {
      "type": "local",
      "command": [
        "npx",
        "@playwright/mcp@latest"
      ]
    },
    "morphllm-fast-apply": {
      "type": "local",
      "command": [
        "npx",
        "@morph-llm/morph-fast-apply"
      ]
    },
    "serena": {
      "type": "local",
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "tavily": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "tavily-mcp@latest"
      ],
      "environment": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "chrome-devtools": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "chrome-devtools-mcp@latest"
      ]
    },
    "antigravity-bridge": {
      "type": "local",
      "command": [
        "node",
        "/home/y_ohi/program/private/antigravity-mcp-bridge/packages/bridge-cli/dist/index.js"
      ],
      "environment": {
        "ANTIGRAVITY_TOKEN": "448fd53f30252a8736827dcf58e1aee23f5f91de4751a81833ab7909daa58767",
        "ANTIGRAVITY_PORT": "8888"
      }
    }
  },
  "lsp": {
    "bash-language-server": {
      "command": [
        "bash-language-server",
        "start"
      ],
      "extensions": [
        ".sh"
      ],
      "disabled": false
    },
    "typescript-language-server": {
      "command": [
        "typescript-language-server",
        "--stdio"
      ],
      "extensions": [
        ".ts"
      ],
      "disabled": false
    },
    "pyright": {
      "command": [
        "pyright-langserver",
        "--stdio"
      ],
      "extensions": [
        ".py"
      ],
      "disabled": false
    }
  },
  "provider": {
    "google-vertex": {
      "models": {
        "gemini-3-flash-preview": {},
        "gemini-3.1-pro-preview": {},
        "gemini-3.1-pro-preview-customtools": {}
      }
    },
    "google-vertex-anthropic": {
      "models": {
        "claude-haiku-4-5@20251001": {},
        "claude-sonnet-4-6@default": {},
        "claude-opus-4-6@default": {}
      }
    },
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": [
          "reasoning.encrypted_content"
        ],
        "store": false
      },
      "models": {
        "gpt-5.3-codex": {
          "name": "GPT 5.3 Codex (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "modalities": {
            "input": [
              "text",
              "image"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "reasoningEffort": "low",
              "reasoningSummary": "auto",
              "textVerbosity": "medium"
            },
            "medium": {
              "reasoningEffort": "medium",
              "reasoningSummary": "auto",
              "textVerbosity": "medium"
            },
            "high": {
              "reasoningEffort": "high",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            },
            "xhigh": {
              "reasoningEffort": "xhigh",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            }
          }
        },
        "gpt-5.2": {
          "name": "GPT 5.2 (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "modalities": {
            "input": [
              "text",
              "image"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "none": {
              "reasoningEffort": "none",
              "reasoningSummary": "auto",
              "textVerbosity": "medium"
            },
            "low": {
              "reasoningEffort": "low",
              "reasoningSummary": "auto",
              "textVerbosity": "medium"
            },
            "medium": {
              "reasoningEffort": "medium",
              "reasoningSummary": "auto",
              "textVerbosity": "medium"
            },
            "high": {
              "reasoningEffort": "high",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            },
            "xhigh": {
              "reasoningEffort": "xhigh",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            }
          }
        },
        "gpt-5.1-codex-max": {
          "name": "GPT 5.1 Codex Max (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "modalities": {
            "input": [
              "text",
              "image"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "reasoningEffort": "low",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            },
            "medium": {
              "reasoningEffort": "medium",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            },
            "high": {
              "reasoningEffort": "high",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            },
            "xhigh": {
              "reasoningEffort": "xhigh",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium"
            }
          }
        }
      }
    }
  },
}
