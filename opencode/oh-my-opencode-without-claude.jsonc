{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",

  // ================================================================================
  // グローバル設定
  // ================================================================================
  "disabled_hooks": [
    "startup-toast"
  ],
  "disabled_mcps": [],

  "agents": {
    // --------------------------------------------------------------------------
    // 1. 指揮・計画レイヤー (Google & OpenAI構成)
    // --------------------------------------------------------------------------

    // [Orchestrator]
    // Claude Opusの代役。100万トークン超の窓を持つGemini 3 Proが最適。
    "orchestrator-sisyphus": {
      "model": "google/antigravity-gemini-3-pro",
      "temperature": 0.5,
      "description": "プロジェクト全体を統括。Geminiの長文脈を活かし、大量のファイルと計画を読み込んで指揮する。"
    },

    // [Planner] Prometheus
    // Claude Thinkingの代役。論理推論で最高スコア(AIME 100%)を出すGPT-5.2を採用。
    "prometheus": {
      "model": "openai/gpt-5.2-thinking", // または gpt-5.2-high-reasoning
      "variant": "high",
      "temperature": 0.3,
      "description": "GPTの強力な推論力を活かし、抜け漏れのない堅牢な計画を策定する。"
    },

    // [Consultant] Metis
    // Planner(GPT)とは異なる視点で監査するため、Googleモデルを採用して多様性を確保。
    "metis": {
      "model": "google/antigravity-gemini-3-pro",
      "category": "ultrabrain",
      "temperature": 0.3
    },

    // --------------------------------------------------------------------------
    // 2. 実行レイヤー (Gemini Main)
    // --------------------------------------------------------------------------

    // [General Executor] Sisyphus
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro",
      "temperature": 0.5
    },

    // [Execution Sub-agent] Sisyphus Junior
    // Claude Sonnetの代役。GPT-5.2は遅延が懸念されるため、Gemini 3 Proを推奨。
    "sisyphus-junior": {
      "model": "google/antigravity-gemini-3-pro",
      "temperature": 0.1,
      "description": "実装担当。Geminiのマルチモーダル理解力を活かし、UI実装も含めて対応。"
    },

    // [Frontend]
    "frontend-ui-ux-engineer": {
      "model": "google/antigravity-gemini-3-pro",
      "temperature": 0.4
    },

    // --------------------------------------------------------------------------
    // 3. 支援・専門家レイヤー
    // --------------------------------------------------------------------------

    // [Oracle]
    // 難問解決にはGPT-5.2の論理力が不可欠。
    "oracle": {
      "model": "openai/gpt-5.2-thinking",
      "category": "ultrabrain",
      "variant": "xhigh", // 最大思考
      "temperature": 0.5
    },

    // [Reviewer] Momus
    // 厳格なレビュアー。
    "momus": {
      "model": "openai/gpt-5.2",
      "temperature": 0.2
    },

    // [Librarian / Explorer]
    // コストパフォーマンス最強のGemini Flashを維持。
    "librarian": {
      "model": "google/antigravity-gemini-3-flash",
      "temperature": 0.1
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
      "temperature": 0.1
    }
  }
}
