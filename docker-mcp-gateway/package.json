{
  "name": "docker-mcp-gateway-manager",
  "version": "1.0.0",
  "description": "Docker MCP Gateway management system using Docker MCP Plugin for simplified MCP server orchestration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "install-plugin": "./install-docker-mcp-plugin.sh",
    "mcp-setup": "./mcp-config.sh setup",
    "mcp-start": "./mcp-config.sh start",
    "mcp-remote": "./mcp-config.sh remote",
    "mcp-check": "./mcp-config.sh check",
    "mcp-show": "./mcp-config.sh show"
  },
  "keywords": [
    "mcp",
    "docker",
    "gateway",
    "model-context-protocol",
    "orchestration"
  ],
  "author": "Docker MCP Gateway Team",
  "license": "MIT",
  "dependencies": {
    "dockerode": "^4.0.2",
    "yaml": "^2.4.1",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15",
    "winston": "^3.13.0",
    "joi": "^17.12.2",
    "axios": "^1.6.8",
    "lodash": "^4.17.21",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/dockerode": "^3.3.23",
    "@types/lodash": "^4.17.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "typescript": "^5.4.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
