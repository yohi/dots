# 各行は「キー=値」の形式で記述します

# Tavilyのシークレットトークン
# tavily.api_token=tvly-YOUR_API_KEY

# Anthropicのシークレットトークン
# anthropic.api_key=YOUR_API_KEY

# Googleのシークレットトークン
# google.api_key=YOUR_API_KEY

# OpenAIのシークレットトークン
# openai.api_key=YOUR_API_KEY

